<h1>Edit a Landmark</h1>
<form method="POST" action="/landmarks/<%=@landmark.id%>">
  <input type="hidden" name="_method" value="patch">
  <label>Landmark Name:</label> <input type="text" name="landmark[name]" id="name" value=<%= @landmark.name %> />

  <br /><br /><br />

  <label>Year Completed:</label> <input type="text" name="landmark[year_completed]" id="year_completed" value=<%= @landmark.year_completed %> />

  <br /><br /><br />

  <input type="submit" value="Edit Landmark" />

</form>
